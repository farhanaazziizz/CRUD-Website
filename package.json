{
  "name": "iso-management-mvp",
  "version": "1.0.0",
  "description": "ISO Certificate Management System MVP with In-App Notifications",
  "main": "index.js",
  "scripts": {
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "cd backend && npm run dev",
    "frontend": "cd frontend && npm run dev",
    "start": "npm run dev",
    "build": "cd frontend && npm run build"
  },
  "keywords": [
    "iso",
    "certificate",
    "management",
    "notification",
    "react",
    "nodejs",
    "sqlite"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}